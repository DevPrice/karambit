    "karambit-inject": "file:..",
    "ts-jest": "^28.0.4",
    "typescript": "^5.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.ts"
    ],
